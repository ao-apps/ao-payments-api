<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- Will not rename since the name helps indicate the format -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]payments[/\\]CreditCard\.java$"
    message="'getExpirationDateMMYY'"
  />

  <!-- Will not reorder overloads -->
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]payments[/\\]CreditCard\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]payments[/\\]TransactionRequest\.java$"
  />

</suppressions>
